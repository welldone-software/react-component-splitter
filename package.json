{
  "name": "react-component-splitter",
  "displayName": "React Component Splitter",
  "description": "Splits long components into shorter, readable subcomponents",
  "version": "0.59.0",
  "icon": "src/assets/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/welldone-software/react-component-splitter"
  },
  "publisher": "SaharAvr",
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Other",
    "Formatters",
    "Snippets"
  ],
  "keywords": [
    "React Component Splitter",
    "React",
    "Split",
    "Component",
    "Code Split",
    "Code Splitter",
    "Component Split",
    "Component Splitter",
    "SubComponent",
    "Sub Component"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "react-component-splitter.split",
        "title": "Split to New Component"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "react-component-splitter.split",
          "group": "1_modification",
          "when": "editorHasSelection && resourceLangId == javascriptreact"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/eslint-plugin": "^7.16.5",
    "@testing-library/jest-dom": "^5.11.9",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.43.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "glob": "^7.1.6",
    "jest": "^26.6.3",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/preset-react": "^7.12.10",
    "eslint": "^8.18.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "find-babel-config": "^1.2.0",
    "fs": "^0.0.1-security",
    "lodash": "^4.17.20",
    "path": "^0.12.7",
    "prettier": "^2.7.1"
  }
}
