{
	"name": "react-component-splitter",
	"displayName": "React Component Splitter",
	"description": "Splits long components into shorter, readable subcomponents",
	"version": "0.7.0",
	"icon": "images/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/welldone-software/react-component-splitter"
	},
	"publisher": "SaharAvr",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other",
		"Formatters",
		"Snippets"
	],
	"keywords": [
		"React Component Splitter",
		"React",
		"Split",
		"Component",
		"Code Split",
		"Code Splitter",
		"Component Split",
		"Component Splitter",
		"SubComponent"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "react-component-splitter.split",
				"title": "Split to New Component"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "react-component-splitter.split",
					"group": "1_modification",
					"when": "editorHasSelection && resourceLangId == javascript"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.43.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@babel/core": "^7.9.0",
		"@babel/preset-react": "^7.9.4",
		"babel-eslint": "^10.1.0",
		"eslint": "^6.8.0",
		"eslint-plugin-react": "^7.19.0",
		"eslint-plugin-unused-imports": "^0.1.2",
		"fs": "^0.0.1-security",
		"path": "^0.12.7",
		"vsce": "^1.75.0",
		"@typescript-eslint/parser": "2.0.0"
	}
}
